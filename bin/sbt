sbtOpts="-Xmx512M -Dfile.encoding=UTF8 -Dsbt.ivy.home=$IVY_HOME -Dsbt.global.base=$SBT_HOME"
echo "SBT options : $sbtOpts"
java $sbtOpts -jar $STRONGLINKS_SANDBOX_DIR/home/bin/sbt-launch.jar "$@"

